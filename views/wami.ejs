<!DOCTYPE html>
<html lang="en" ng-app="wami">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="<%= desc %>">
    <meta name="author" content="">

    <title><%= title %> - WAMI Multiple-Target Tracking</title>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.0/angular.min.js"></script>
    <script src="js/controller.js"></script>
    <script type="text/javascript" src="js/wheelscrollzoom.js"></script>
    <!-- Bootstrap Core CSS -->
    <link rel="stylesheet" href="css/bootstrap.min.css" type="text/css">

    <!-- Custom Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="font-awesome/css/font-awesome.min.css" type="text/css">

    <!-- Plugin CSS -->
    <link rel="stylesheet" href="css/animate.min.css" type="text/css">

    <!-- Custom CSS -->
    <link rel="stylesheet" href="css/creative.css" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    <style type="text/css">
    /* CSS used here will be applied after bootstrap.css */
        .carousel {
            margin-bottom: 20px;
        }
        .item .thumb {
        	width: 12.5%;
        	cursor: pointer;
        	float: left;
        }
        .item .thumb img {
        	width: 96%;
        	margin: 2px;
        }
        .item img {
        	width: 100%;	
        }
        .project-name{
            margin-top: 40px;
            margin-bottom: 40px;
            font-size: 35px;
            color: red;
        }
        .img-responsive{
            margin-top: 20px;
        }
        h2{
            font-size: 23px;
            text-align: left;
        }
        h3{
            font-size: 19px;
        }
        .login-setting{
            margin-top: 60px;
        }
        .carousel-caption{
            text-align: right;
        }
        .thumbcarousel-caption{
            text-align: center;
            font-weight: bold;
            font-family: "Comic Sans", Georgia;
        }
       .list-group{
            text-align: center;
            font-weight: bold;
            font-family: "Comic Sans", Georgia;
            font-size: 20px;
        }
        .list-group-item {
            border-radius: 5px;
            margin: 15px;
        }
        #rawimagelayout{
            text-align: center;
            font-weight: bold;
            font-family: "Comic Sans", Georgia;
            font-size: 17px;
            margin-bottom: 15px;
        }
        a.control-btn:focus{
            border: solid;
            /*opacity: .5;*/
        }

        
    </style>

</head>

<body id="page-top" ng-controller="WamiController">

<% //include partials/topnav %>

<div class="page-wrapper container">
<div class="top-nav">
    <div class="row">
        <div class="col-xs-2">
            <img src="/img/IFT_image.jpg" class="img-responsive">
        </div>
        <div class="col-xs-8">
            <h1 class="text-center project-name">WAMI Multiple-Target Tracking</h1>
        </div>
        <div class="col-xs-2">
            <a href="#" class="btn btn-md btn-primary login-setting">User Login</a>
        </div>
    </div>
</div>

<br />
<br />

<!--upload form-->

    <div>
        <form class="form-inline">
            <h3>Load raw images</h3>
             <div class="form-group">
                <input type="file" class="form-control" id="exampleInputFile" multiple>
              </div>
             <div class="form-group">
                <input type="submit" class="form-control btn btn-primary" value="Submit"/>
              </div>              
        </form>
    </div>


<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-10">
        <h2>Display</h2>
        <div id="carousel" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner">
                <div class="item" ng-class="{active: $index==0}" ng-repeat="img in curImages">
                    <img class="wheelscroll" src={{img.url}}>
                    <div class="carousel-caption">
                        <h3>Pic {{$index+1}}</h3>
                        <!--<p>This is the 1st pic</p>-->
                    </div>
                </div>
            </div>
            
          <!-- Controls -->
          <a class="left carousel-control" href="#carousel" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="right carousel-control" href="#carousel" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>            
            
        </div> 
    <div class="clearfix">
        <div id="thumbcarousel" class="carousel slide" data-interval="false">
            <div class="carousel-inner">
                <div class="item active">
                    <div data-target="#carousel" data-slide-to="{{$index}}" class="thumb" ng-repeat="img in curImages track by $index">
                        <img class="image-thumb" src={{img.url}}>
                        <div class="thumbcarousel-caption">
                            <p>{{$index+1}}</p>
                        </div>
                    </div>
                </div><!-- /item -->
            </div><!-- /carousel-inner -->
            <!--<a class="left carousel-control" href="#thumbcarousel" role="button" data-slide="prev">-->
            <!--    <span class="glyphicon glyphicon-chevron-left"></span>-->
            <!--</a>-->
            <!--<a class="right carousel-control" href="#thumbcarousel" role="button" data-slide="next">-->
            <!--    <span class="glyphicon glyphicon-chevron-right"></span>-->
            <!--</a>-->
        </div> <!-- /thumbcarousel -->
    </div><!-- /clearfix -->

    </div>
    <div class="col-xs-12 col-sm-12 col-md-2">
            <h2>Controls</h2>
            <div class="row">
                <div class="list-group">
                  <!--<a href="#" class="list-group-item list-group-item-success">-->
                  <!--  Load Raw Images-->
                  <!--</a>-->
                  <a href="#" class="list-group-item list-group-item-info control-btn" ng-click="setImages(1)">Original</a>
                  <a href="#" class="list-group-item list-group-item-danger control-btn" ng-click="setImages(2)">Registration</a>
                  <a href="#" class="list-group-item list-group-item-success control-btn" ng-click="setImages(3)">Detection</a>
                  <a href="#" class="list-group-item list-group-item-warning control-btn" ng-click="setImages(4)">Association</a>
                </div>            
            </div>
    </div>
    
</div>


</div>

    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

    <!-- Plugin JavaScript -->
    <script src="js/jquery.easing.min.js"></script>
    <script src="js/jquery.fittext.js"></script>
    <script src="js/wow.min.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="js/creative.js"></script>    

    <script>
        //wheelzoom(document.querySelector('img.wheelscroll'));
        $(document).ready(function(){
            $('img.wheelscroll').click(function(){
                //console.log();
                wheelzoom(this);
            })
        });
      
    </script>
    

</body>

</html>
